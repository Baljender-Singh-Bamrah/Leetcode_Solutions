-> Approach 1: Using reverse function
class Solution {
public:
    void reverseString(vector<char>& s) {
        reverse(s.begin(),s.end());
    }
};

-> Approach 2: Swapping
class Solution {
public:
    void reverseString(vector<char>& s) {
        int l=0 , r=s.size()-1 ;
        while(l<r)
        {
            swap(s[r--],s[l++]);
        } 
    }
};
