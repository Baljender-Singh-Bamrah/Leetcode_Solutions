class Solution {
public:
    TreeNode* lowestCommonAncestor(TreeNode* root, TreeNode* p, TreeNode* q) {
        if(!root) return NULL;

        TreeNode *temp = root;
        while(temp) {
            if(p->val < temp->val && q->val<temp->val) temp = temp->left;
            else if(p->val > temp->val && q->val>temp->val)  temp = temp->right;
            else return temp;
        }
        return root;
    }
};
