class Solution {
public:
    vector<int> findErrorNums(vector<int>& nums) {
        vector<int> mp(nums.size(), 0);
        for(int& it : nums) {
            mp[it-1]++;
        }
        vector<int> result(2,0);
        for(int i=0 ; i<nums.size(); i++) {
            if(mp[i]>1) result[0] = i+1;
            else if(mp[i]==0) result[1] = i+1;
        }

        return result;
    }
};
