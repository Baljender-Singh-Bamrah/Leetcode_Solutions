class Solution {
public:
    int areaOfMaxDiagonal(vector<vector<int>>& dimensions) {
        int max_area = 0, len = 0;
        for(auto &it: dimensions) {
            int area = (it[0]*it[0]) + (it[1]*it[1]);
            int  l = it[0]*it[1] ;
            if(area>max_area || area == max_area && len < l) {
                max_area = area;
                len = l;
            }
        }
        return len;
    }
};
