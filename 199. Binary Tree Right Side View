class Solution {
public:
    vector<int> rightSideView(TreeNode* root) {
        if(!root) return {};
        vector<int> result;
        deque<TreeNode*> q;
        q.push_back(root);
        while(!q.empty()) {
            result.push_back(q.back()->val);
            int n = q.size();
            for(int i=0 ; i<n ; i++) {
                TreeNode * node = q.front();
                q.pop_front();

                if(node->left) q.push_back(node->left);
                if(node->right) q.push_back(node->right);
            }
        }
        return result;
    }
};
