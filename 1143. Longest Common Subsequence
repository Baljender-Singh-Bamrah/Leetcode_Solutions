class Solution {
public:
    int longestCommonSubsequence(string text1, string text2) {
        vector<int> dp(text1.size(), 0);
        int longest = 0;

        for(auto &it: text2) {
            int curr = 0;
            for(int i=0 ; i<dp.size() ; i++) {
                if(curr < dp[i]) {
                    curr = dp[i];
                } else if( it == text1[i] ) {
                    dp[i] = curr + 1;
                    longest = max(longest, dp[i]);
                }
            }
        }   
        return longest;  
    }
};
